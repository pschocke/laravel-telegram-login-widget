{
    "name": "pschocke/laravel-telegram-login-widget",
    "description": "Easily integrate Telegrams login widget into your Laravel application to send Telegram messages",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Schocke",
            "email": "patrick@ausbildung-ms.de",
            "homepage": "https://github.com/pschocke/"
        }
    ],
    "homepage": "https://github.com/pschocke/telegramloginwidget",
    "keywords": ["Laravel", "TelegramLoginWidget"],
    "scripts": {
        "test": "phpunit --color=always"
    },
    "require": {
        "illuminate/support": "~9",
        "tg/tgwebvalid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~7",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "pschocke\\TelegramLoginWidget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pschocke\\TelegramLoginWidget\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pschocke\\TelegramLoginWidget\\TelegramLoginWidgetServiceProvider"
            ],
            "aliases": {
                "MyPackage": "pschocke\\TelegramLoginWidget\\Facades\\MyPackage"
            }
        }
    }
}
